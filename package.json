{
  "name": "jira-time-tracker",
  "version": "0.1.0",
  "description": "Time tracker for JIRA issues",
  "main": "main.js",
  "scripts": {
    "build": "npm run clean && vite build",
    "clean": "rm -rf dist",
    "dev": "vite",
    "serve": "vite preview",
    "start": "electron .",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "jest -c tests/e2e/jest.config.ts --runInBand",
    "test:unit": "jest -c tests/unit/jest.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drawkh/jira-time-tracker.git"
  },
  "keywords": [
    "jira",
    "time-tracker"
  ],
  "author": "Gilles GAUTIER",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drawkh/jira-time-tracker/issues"
  },
  "homepage": "https://github.com/drawkh/jira-time-tracker#readme",
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vitejs/plugin-vue": "^1.9.3",
    "electron": "^15.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.3.2",
    "spectron": "^15.0.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "vite": "^2.6.10"
  },
  "dependencies": {
    "vue": "^3.2.20"
  }
}
